      "voices": [
        "paimon-morning-1.wav",
        "paimon-morning-2.wav",
        "paimon-morning-3.wav"
      ]
    },
    {
      "event": "$time_noon",
      "voices": [
        "paimon-noon-1.wav"
      ]
    },
    {
      "event": "$time_midnight",
      "voices": [
        "paimon-midnight-1.wav"
      ]
    },
    {
      "event": "$time_each_hour",
      "voices": [
        "paimon-rest-1.wav",
        "paimon-rest-2.wav"
      ]
    }
