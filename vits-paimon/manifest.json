      "voices": [
        "paimon-try-2.wav"
      ]
    },
    {
      "keywords": "hello",
      "voices": [
        "paimon-hello-world-1.wav",
        "paimon-hello-world-2.wav"
      ]
    },
    {
      "keywords": ["for", "while"],
      "voices": [
        "paimon-loop-2.wav",
        "paimon-loop-3.wav"
      ]
    },
    {
      "keywords": ["await", "wait", "sleep"],
      "voices": [
        "paimon-await-1.wav",
        "paimon-await-2.wav"
      ]
    },
    {
      "keywords": ["mian", "ture", "flase", "viod", "（", "）", "‘", "’", "“", "”", "，", "；"],
      "voices": [
        "paimon-mistake-1.wav",
        "paimon-mistake-2.wav"
      ]
    },
    {
      "keywords": ["v-html"],
      "voices": [
        "paimon-danger-1.wav",
        "paimon-danger-2.wav"
      ]
    },
